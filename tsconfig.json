{
    "compilerOptions": {
        "target": "es2015",
        "module": "commonJS",
        "outDir": "./dist",
        // "declaration": true,
        "strict": true,
        "sourceMap": false,
        "skipLibCheck": true,
        "strictNullChecks": true,
        "noImplicitThis": true,
        "moduleResolution": "node",
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "src"
    ],
    "exclude": [
        "mode_modules"
    ]
}
