{
  "name": "mckismetlab-discord-bot",
  "version": "1.2.0",
  "description": "mcKismetLab Discord Bot",
  "author": "Quasi (https://github.com/QuasiMkl)",
  "license": "GPL-3.0",
  "dependencies": {
    "axios": "^0.21.1",
    "cross-env": "^7.0.3",
    "discord-buttons": "^4.0.0",
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "fs-extra": "^9.1.0",
    "got": "^11.8.2",
    "moment": "^2.29.1",
    "socket.io": "^3.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.6",
    "@types/node": "^15.12.2",
    "@types/socket.io": "^2.1.12",
    "@types/uuid": "^8.3.1",
    "@types/ws": "^7.4.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./build/botMain.js",
    "start:dev": "cross-env NODE_ENV=development node --trace-warnings ./build/botMain.js",
    "build": "yarn outputVersion && node build.js && tsc",
    "dev": "cross-env NODE_ENV=development ts-node-dev ./src/botMain.ts",
    "debug": "cross-env NODE_ENV=development ts-node-dev --inspect-brk=5858 -- .",
    "outputVersion": "node -p \"'export const API_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts"
  }
}
